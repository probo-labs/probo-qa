{
  "name": "probo-qa",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "engines": {
    "node": ">=20.19.5",
    "pnpm": ">=10.18.3"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && pnpm type-check && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "playwright test --grep-invert 'Build Smoke Tests' --reporter=list",
    "test:build": "playwright test --config=playwright.build.config.ts --grep 'Build Smoke Tests' --reporter=list",
    "test:all": "pnpm test:build && pnpm test"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "next": "15.5.3",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.0",
    "@probolabs/highlighter": "^1.0.12",
    "playwright": "^1.56.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
